<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.0</TargetFramework>
    <Version>0.0.1-alpha</Version>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Authors>Andrei Fangli</Authors>
    <Company />
    <Description>Markup Parser (Mup) is an extensible .NET Standard Library (1.0) containing types for parsing and generating HTML from text using the Creole lightweight markup language.</Description>
    <PackageId>Mup</PackageId>
    <PackageTags>markup parser creole net standard html portable library</PackageTags>
    <PackageRequireLicenseAcceptance>False</PackageRequireLicenseAcceptance>
    <PackageProjectUrl>https://bitbucket.org/Andrei15193/mup/</PackageProjectUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://bitbucket.org/Andrei15193/mup.git</RepositoryUrl>
    <Copyright>Andrei Fangli</Copyright>
    <PackageLicenseUrl>https://bitbucket.org/Andrei15193/mup/wiki/License</PackageLicenseUrl>
    <PackageIconUrl>https://mupstore.blob.core.windows.net/nuget/logo.png</PackageIconUrl>
  </PropertyGroup>

</Project>